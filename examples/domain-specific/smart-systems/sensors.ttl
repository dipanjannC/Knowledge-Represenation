@prefix : <http://www.semanticweb.org/ontologies/2024/11/SmartHomeOnto#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# ========================================
# SENSOR TYPES
# ========================================

:MotionSensor rdf:type owl:Class ;
    rdfs:subClassOf :Sensor ;
    rdfs:comment "Sensor that detects movement" .

:TemperatureSensor rdf:type owl:Class ;
    rdfs:subClassOf :Sensor ;
    rdfs:comment "Sensor that measures ambient temperature" .

:LightSensor rdf:type owl:Class ;
    rdfs:subClassOf :Sensor ;
    rdfs:comment "Sensor that measures light intensity" .
