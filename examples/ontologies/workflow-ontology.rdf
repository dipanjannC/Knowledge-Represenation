<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:workflow="http://example.org/workflow#"
         xml:base="http://example.org/workflow#">

  <!-- Ontology Declaration -->
  <owl:Ontology rdf:about="">
    <rdfs:label>Workflow Ontology</rdfs:label>
    <rdfs:comment>An ontology for modeling agentic workflows and processes</rdfs:comment>
  </owl:Ontology>

  <!-- Classes -->
  <owl:Class rdf:ID="Workflow">
    <rdfs:label>Workflow</rdfs:label>
    <rdfs:comment>A sequence of connected steps or tasks</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:ID="Step">
    <rdfs:label>Step</rdfs:label>
    <rdfs:comment>An individual step within a workflow</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:ID="AutomatedStep">
    <rdfs:subClassOf rdf:resource="#Step"/>
    <rdfs:label>Automated Step</rdfs:label>
    <rdfs:comment>A step that can be executed automatically by an agent</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:ID="ManualStep">
    <rdfs:subClassOf rdf:resource="#Step"/>
    <rdfs:label>Manual Step</rdfs:label>
    <rdfs:comment>A step that requires human intervention</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:ID="Condition">
    <rdfs:label>Condition</rdfs:label>
    <rdfs:comment>A condition that must be met for a step to execute</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:ID="Agent">
    <rdfs:label>Agent</rdfs:label>
    <rdfs:comment>An entity capable of executing workflow steps</rdfs:comment>
  </owl:Class>

  <!-- Object Properties -->
  <owl:ObjectProperty rdf:ID="hasStep">
    <rdfs:domain rdf:resource="#Workflow"/>
    <rdfs:range rdf:resource="#Step"/>
    <rdfs:label>has step</rdfs:label>
    <rdfs:comment>Links a workflow to its constituent steps</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="nextStep">
    <rdfs:domain rdf:resource="#Step"/>
    <rdfs:range rdf:resource="#Step"/>
    <rdfs:label>next step</rdfs:label>
    <rdfs:comment>Indicates the next step in sequence</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="executedBy">
    <rdfs:domain rdf:resource="#Step"/>
    <rdfs:range rdf:resource="#Agent"/>
    <rdfs:label>executed by</rdfs:label>
    <rdfs:comment>Indicates which agent executes a step</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="hasCondition">
    <rdfs:domain rdf:resource="#Step"/>
    <rdfs:range rdf:resource="#Condition"/>
    <rdfs:label>has condition</rdfs:label>
    <rdfs:comment>Associates a step with its execution conditions</rdfs:comment>
  </owl:ObjectProperty>

  <!-- Data Properties -->
  <owl:DatatypeProperty rdf:ID="stepName">
    <rdfs:domain rdf:resource="#Step"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label>step name</rdfs:label>
    <rdfs:comment>The name of a workflow step</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:ID="stepOrder">
    <rdfs:domain rdf:resource="#Step"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label>step order</rdfs:label>
    <rdfs:comment>The order position of a step in the workflow</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:ID="estimatedDuration">
    <rdfs:domain rdf:resource="#Step"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#duration"/>
    <rdfs:label>estimated duration</rdfs:label>
    <rdfs:comment>Expected time to complete the step</rdfs:comment>
  </owl:DatatypeProperty>

  <!-- Example Instances -->
  <Workflow rdf:ID="dataAnalysisWorkflow">
    <rdfs:label>Data Analysis Workflow</rdfs:label>
    <rdfs:comment>A workflow for analyzing customer data</rdfs:comment>
    <hasStep rdf:resource="#dataCollection"/>
    <hasStep rdf:resource="#dataCleaning"/>
    <hasStep rdf:resource="#dataAnalysis"/>
    <hasStep rdf:resource="#reportGeneration"/>
  </Workflow>

  <AutomatedStep rdf:ID="dataCollection">
    <stepName>Data Collection</stepName>
    <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
    <estimatedDuration rdf:datatype="http://www.w3.org/2001/XMLSchema#duration">PT30M</estimatedDuration>
    <nextStep rdf:resource="#dataCleaning"/>
  </AutomatedStep>

  <AutomatedStep rdf:ID="dataCleaning">
    <stepName>Data Cleaning</stepName>
    <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
    <estimatedDuration rdf:datatype="http://www.w3.org/2001/XMLSchema#duration">PT1H</estimatedDuration>
    <nextStep rdf:resource="#dataAnalysis"/>
  </AutomatedStep>

  <ManualStep rdf:ID="dataAnalysis">
    <stepName>Data Analysis</stepName>
    <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
    <estimatedDuration rdf:datatype="http://www.w3.org/2001/XMLSchema#duration">PT4H</estimatedDuration>
    <nextStep rdf:resource="#reportGeneration"/>
  </ManualStep>

  <AutomatedStep rdf:ID="reportGeneration">
    <stepName>Report Generation</stepName>
    <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
    <estimatedDuration rdf:datatype="http://www.w3.org/2001/XMLSchema#duration">PT15M</estimatedDuration>
  </AutomatedStep>

</rdf:RDF>